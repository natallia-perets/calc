namespace Calculator.Operators
{
    class DivideOperator : IOperator
    {
        public double Apply(double a, double b)
        {
            return a / b;
        }

        string IOperator.Key
        {
            get { return '/'; }
            set { throw new System.NotImplementedException(); }
        }
        public override string ToString()
        {
            return "/";
        }
    }
}
